<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <record id="run_telegram_commands_cache_updates" model="ir.actions.server">
        <field name="name">Updates cache for bound telegram commands of some model</field>
        <field name="model_id" ref="model_telegram_command"/>
        <field name="condition">True</field>
        <field name="type">ir.actions.server</field>
        <field name="state">code</field>
        <field name="code">self.update_cache_bus_message(cr, uid, context['active_ids'], context=context)</field>
    </record>
</data>
</openerp>
